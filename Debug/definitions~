(defun cddr (ENV) (cdr (cdr ENV)))
(defun caar (ENV) (car (car ENV)))
(defun cadr (ENV) (car (cdr ENV)))
(defun cdar (ENV) (cdr (car ENV)))
(print (cddr (list 1 2 3 4 5))) 
(print (caar (list (list 1 2 3) 4 5))) 
(print (cadr (list 1 2 3 4 5))) 
(print (cdar (list 1 2 3 4 5))) 
(defun condicional (ENV) (if ENV (caar (list ENV 2 3 4 5)) 1234))
(print (condicional (list 1 2 3 4)))
